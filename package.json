{
  "name": "synonym-finder",
  "displayName": "synonym-finder",
  "description": "finds synonyms",
  "version": "0.0.1",
  "publisher": "rediffusion",

  "homepage": "https://github.com/rediffusion/synonym-finder/blob/master/README.md",
  "repository": {
	  "type": "git",
	  "url": "https://github.com/rediffusion/synonym-finder"
  },

  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "find synonyms"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.4",
    "node-fetch": "^2.6.1"
  }
}
